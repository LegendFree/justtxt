package javapract;
import java.util.*;

/**
 *
 * @author SAHU
 */
public class factorial {
    public static void main(String a[]){
        Scanner sc=new Scanner (System.in);
        int ans=1,n;
        System.out.println("Enter the number");
        n=sc.nextInt();
        while(n>0){
            ans=ans*n;
            n=n-1;
        }
        System.out.println("The factorial of is:"+ ans);      
        
    }
    
}
